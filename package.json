{
  "name": "coffee-cli",
  "version": "1.0.0",
  "description": "JSON coffee journal CLI",
  "main": "index.js",
  "scripts": {
    "test": "ava --watch"
  },
  "bin": {
    "coffee": "index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuscriptmastr/coffee-cli.git"
  },
  "ava": {
    "nodeArguments": [
      "--experimental-specifier-resolution=node"
    ]
  },
  "keywords": [
    "coffee",
    "json-schema",
    "journal",
    "pourover",
    "chemex",
    "v60",
    "kalita-wave",
    "percolation",
    "immersion",
    "cupping",
    "cli"
  ],
  "author": "Joshua Martin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manuscriptmastr/coffee-cli/issues"
  },
  "homepage": "https://github.com/manuscriptmastr/coffee-cli#readme",
  "dependencies": {
    "dayjs": "^1.9.7",
    "json-schema-defaults": "^0.4.0",
    "json-schema-ref-parser": "^9.0.6",
    "ramda": "^0.27.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "ava": "^3.14.0"
  }
}
