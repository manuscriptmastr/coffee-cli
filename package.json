{
  "name": "pourover-cli",
  "version": "1.0.0",
  "description": "JSON coffee journal CLI for percolation brews",
  "main": "index.js",
  "scripts": {
    "test": "ava --watch"
  },
  "bin": {
    "pourover": "index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuscriptmastr/pourover-cli.git"
  },
  "ava": {
    "nodeArguments": [
      "--experimental-specifier-resolution=node"
    ]
  },
  "keywords": [
    "coffee",
    "json-schema",
    "journal",
    "pourover",
    "chemex",
    "v60",
    "kalita-wave",
    "percolation",
    "cli"
  ],
  "author": "Joshua Martin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manuscriptmastr/pourover-cli/issues"
  },
  "homepage": "https://github.com/manuscriptmastr/pourover-cli#readme",
  "dependencies": {
    "dayjs": "^1.8.36",
    "ramda": "^0.27.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/yargs": "^15.0.5",
    "ava": "^3.12.1"
  }
}
